                <ul class="sidebar-menu" data-widget="tree">
                    @if (Model != null)
                    {
                        foreach (var item in Model.Where(s => s.ParentID == 0).GroupBy(Obj => new { Obj.NodeID }).Distinct().ToList())
                        {
                            <li class="treeview">
                                <a href="@item.FirstOrDefault().destination" title="@item.FirstOrDefault().NodeID">
                                    <i class="fa fa-share"></i> <span> @item.FirstOrDefault().NodeName</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    @foreach (var firstItem in (Model.Where(s => s.ParentID == item.FirstOrDefault().NodeID).ToList()))
                                    {
                                        <li>
                                            <a href="@firstItem.destination" title="@firstItem.NodeID" onclick="fetchNodeId(this)">
                                                <i class="fa fa-circle-o"></i> <span> @firstItem.NodeName</span>
                                                <span class="pull-right-container">
                                                    <i class="fa fa-angle-left pull-right"></i>
                                                </span>
                                            </a>
                                            <ul class="treeview-menu">
                                                @foreach (var secondItem in (Model.Where(s => s.ParentID == firstItem.NodeID).ToList()))
                                                {
                                                    <li>
                                                        <a href="@secondItem.destination" title="@secondItem.destination" onclick="fetchNodeId(this)">
                                                            <i class="fa fa-circle-o"></i> <span> @secondItem.NodeName</span>
                                                            @*<span class="pull-right-container">
                                                                <i class="fa fa-angle-left pull-right"></i>
                                                            </span>*@
                                                        </a>
                                                        <ul>
                                                            @foreach (var ThirdItem in (Model.Where(s => s.ParentID == secondItem.NodeID).ToList()))
                                                            {
                                                                <li>
                                                                    <a href="@ThirdItem.destination" title="@ThirdItem.NodeID" onclick="fetchNodeId(this)">
                                                                        @ThirdItem.NodeName
                                                                    </a>
                                                                    <ul>
                                                                        @foreach (var FourthItem in (Model.Where(s => s.ParentID == ThirdItem.NodeID).ToList()))
                                                                        {
                                                                            <li>
                                                                                <a href="@FourthItem.destination" title="@FourthItem.NodeID" onclick="fetchNodeId(this)">
                                                                                    @FourthItem.NodeName
                                                                                </a>
                                                                                <ul>
                                                                                    @foreach (var FifthItem in (Model.Where(s => s.ParentID == FourthItem.NodeID).ToList()))
                                                                                     {
                                                                                          <li>
                                                                                               <a href="@FifthItem.destination" title="@FifthItem.NodeID" onclick="fetchNodeId(this)">
                                                                                                    @FifthItem.NodeName
                                                                                               </a>
                                                                                          </li>
                                                                                      }
                                                                                </ul>
                                                                            </li>
                                                                        }
                                                                    </ul>
                                                                </li>
                                                            }
                                                        </ul>

                                                    </li>

                                                }
                                            </ul>

                                        </li>
                                    }
                                </ul>
                            </li>

                        }
                    }

                </ul>
