                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Jivo </b>CallCenter</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top navbar-fixed-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                         @*Tasks: style can be found in dropdown.less*@ 
                        <li class="dropdown tasks-menu topBar">
                            <a class="dropdown-toggle" data-toggle="tooltip" data-placement="bottom" title="Un-Approved Shops" href='#'>
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-danger" id="unapprovedSalesCount">0</span>
                            </a>
                        </li>
                        @*<li class="dropdown tasks-menu topBar">
                            <a class="dropdown-toggle" href='#'>
                                <i class="fa fa-flag-o"></i>

                            </a>
                        </li>*@
                        <!-- User Account: style can be found in dropdown.less -->
