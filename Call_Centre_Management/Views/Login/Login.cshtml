                        $('#top20CancelOrder').append(val);
                    }
                }

            })
        });
    }


    function LoadApproval() {
        $.ajax({
            url: '/Login/Approvaldata',
            method: 'Get',
            success: function (data) {

                if (data.listPendingOrder.length > 0) {

                    $('#PendingOrderbody').empty();
                    for (var x = 0; x < data.listPendingOrder.length; x++) {
                        var PendingBody = '<tr><td>' + data.listPendingOrder[x]["id"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["orderid"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["custMobile"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["custName"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["totalAmount"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["totalQty"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["Address"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["Remark"] + '</td>' +
                            '<td>' + data.listPendingOrder[x]["insertedDate"] + '</td></tr>'
                        $('#PendingOrderbody').append(PendingBody);
                    }
                }

                if (data.listApprovedOrder.length > 0) {
                    $('#ApprovedOrder').empty();
                    for (var m = 0; m < data.listApprovedOrder.length; m++) {
                        var ApprovedBody = '<tr> <td>' + data.listApprovedOrder[m]["id"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["orderid"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["custMobile"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["custName"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["totalAmount"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["totalQty"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["Address"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["Remark"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["insertedDate"] + '</td>' +
                            '<td>' + data.listApprovedOrder[m]["ApprovalRemark"] + '</td></tr>'

                        $('#ApprovedOrder').append(ApprovedBody);
                    }
                }

                if (data.listNotApprovedOrder.length > 0) {
                    $('#NotApprovedOrder').empty();
                    for (var n = 0; n < data.listNotApprovedOrder.length; n++) {
                        var NotApprovedBody = '<tr><td>' + data.listNotApprovedOrder[n]["id"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["orderid"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["custMobile"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["custName"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["totalAmount"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["totalQty"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["Address"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["Remark"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["insertedDate"] + '</td>' +
                            '<td>' + data.listNotApprovedOrder[n]["ApprovalRemark"] + '</td></tr>'
                        $('#NotApprovedOrder').append(NotApprovedBody);
                    }
                }
            }
        });
    }
