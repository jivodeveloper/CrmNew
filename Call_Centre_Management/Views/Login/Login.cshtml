    function getOrder(id) {
        $.ajax({
            url: "/Order/TotalOrder",
            method: "Post",
            data: { id: id },
            success: function (data) {
                $('#dailyOrder').html(data.todayTotalOrder);
                $('#monthlyOrder').html(data.monthlyTotalOrder);

                $('#tbl_monthOrder').empty();
                var header = '<thead>' +
                    '<tr class="info" >' +
                    '<th>Position</th>' +
                    '<th>Name</th>' +
                    '<th>Order</th>' +
                    '</tr >' +
                    '</thead>'
                $('#tbl_monthOrder').append(header);

                if (data.MonthEmpPer.length > 0) {
                    for (var i = 0; i < data.MonthEmpPer.length; i++) {
                        var j = parseInt(i) + 1;
                        var val = '<tbody>' + '<tr>' +
                            '<td>' + j + '</td>' +
                            '<td>' + data.MonthEmpPer[i]["empName"] + '</td>' +
                            '<td>' + data.MonthEmpPer[i]["totalOrder"] + '</td>' +
                            '</tr>' //+ '</table>'
                        $('#tbl_monthOrder').append(val);
                    }
                }

                $('#tbl_todayOrder').empty();
                var header1 = '<thead>' +
                    '<tr class="info">' +
                    '<th>Position</th>' +
                    '<th>Name</th>' +
                    '<th>Order</th>' +
                    '</tr >' +
                    '</thead>'
                $('#tbl_todayOrder').append(header1);

                if (data.TodayEmpPer.length > 0) {
                    for (var ii = 0; ii < data.TodayEmpPer.length; ii++) {
                        var jj = parseInt(ii) + 1;
                        var val = '<tbody>' + '<tr>' +
                            '<td>' + jj + '</td>' +
                            '<td>' + data.TodayEmpPer[ii]["empName"] + '</td>' +
                            '<td>' + data.TodayEmpPer[ii]["totalOrder"] + '</td>' +
                            '</tr>' //+ '</table>'
                        $('#tbl_todayOrder').append(val);
                    }
                }

            }
        });
    }

    function fetchDailyOrderCategory() {
