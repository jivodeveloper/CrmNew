                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',

                            dataLabels: {
                                enabled: true,
                                format: '<b>{point.y:f}</b>',
                                distance: -20
                            },
                            showInLegend: true
                        }
                    },
                    series: [{
                        name: 'Sale',
                        colorByPoint: true,
                        data: data
                    }]
                });
            }
        });
    }

