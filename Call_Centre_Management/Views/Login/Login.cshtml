    function Displaycategorywiseitem(Getcategoryname) {
        $('#loading').show();
        //var GetCategoryValue = $(Getcategoryname).attr("value");
        var GetCategoryValue = $(Getcategoryname).html();
        $.ajax({
            url: "/Order/OrderMonthlywithcategoryList",
            method: "Post",
            data: { GetCategoryName: GetCategoryValue },
            success: function (Result) {
                $('#loading').fadeOut();
                $("#DisplayMonthlyItem").show();
                $('#tblMonthlyOrders').empty();
                $('#DisplayCategoryName').text('');

                var header = '<thead>' +
                    '<tr class="info">' +
                    //'<th>Item Id</th>' +
                    '<th>Item Name</th>' +
                    '<th>Item Quantity</th>' +
                    '<th>Item Rate</th>' +
                    '<th>Item Total Amount</th>' +
                    '</tr >' +
                    '</thead>'
                $('#tblMonthlyOrders').append(header);

                $.each(Result, function (index, value) {
                    // $('#DisplayCategoryName').html("<h4 style='text-align:center'>" + value.CategoryName + "</h4>");
                    $('#DisplayCategoryName').html(value.CategoryName + ' ' + '<button   type="button" class="close" onclick="Removepaneldiv()">&times;</button>');
                    $("#tblMonthlyOrders").append(
                        '<tr>' +
                        //'<td>' + value.ItemId + '</td>' +
                        '<td>' + value.item_name + '</td>' +
                        '<td>' + value.item_qty + '</td>' +
                        '<td>' + value.item_rate + '</td>' +
                        '<td>' + value.item_total_amount + '</td>' +
                        '</tr>'
                    );
                });
            }
        });
    }

    function Removepaneldiv() {
        $("#DisplayMonthlyItem").hide(1000);
        $('#tblMonthlyOrders').empty();
        $('#DisplayCategoryName').text('');
