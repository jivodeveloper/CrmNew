    function GetOrderStatusCount(id) {
        //var id = $('#ddStateTotalSale').val();
        $.ajax({
            url: "/Order/GetOrderStatusCountList",
            method: "Get",
            data: { id: id },
            success: function (data) {
                $('#tbl_PandingOrder').empty();

                var header = '<thead>' +
                    '<tr class="info" >' +
                    '<th>No</th>' +
                    '<th>Days</th>' +
                    '<th>Pending Sum of Quantity</th>' +
                    '<th>Assign Order</th>' +
                    '</tr >' +
                    '</thead>'
                $('#tbl_PandingOrder').append(header);

                for (var i = 0; i < data.GetStatus.length; i++) {
                    var j = parseInt(i) + 1;
                    var val = '<tbody>' + '<tr>' +
                        '<td>' + j + '</td>' +
                        '<td>' + data.GetStatus[i]["Pendingdays"] + '</td>' +
                        '<td>' + data.GetStatus[i]["pandingSumofqty"] + '</td>' +
                        '<td>' + data.GetStatus[i]["AssignOrder"] + '</td > ' +
                        '</tr>'
                    $('#tbl_PandingOrder').append(val);
                }

                $('#tbl_DeliveryOrder').empty();

                var headers = '<thead>' +
                    '<tr class="info" >' +
                    '<th>No</th>' +
                    '<th>Days</th>' +
                    '<th>Delivery Sum of Quantity</th>' +
                    '</tr >' +
                    '</thead>'
                $('#tbl_DeliveryOrder').append(headers);
                for (var j = 0; j < data.GetStatuss.length; j++) {
                    var k = parseInt(j) + 1;
                    var vals = '<tbody>' + '<tr>' +
                        '<td>' + k + '</td>' +
                        '<td>' + data.GetStatuss[j]["Deliverydays"] + '</td>' +
                        '<td>' + data.GetStatuss[j]["DeliverySumofqty"] + '</td>' +
                        '</tr>'
                    $('#tbl_DeliveryOrder').append(vals);
                }
            }
        });
    }

