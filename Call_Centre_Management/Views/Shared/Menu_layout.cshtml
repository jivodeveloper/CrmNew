        //        .filter(':containsi("' + query + '")')
        //        .show();
        //}
        $(function () {

            //$(document).tooltip();
            $('input, textArea').bind('focusout', function () {
                var x = $(this).val().toUpperCase();
                $(this).val(x);
            });

            $('#loader').hide();
            $('#loader1').hide();

            $('.table').tablesorter();

            $(".info").css("cursor-pointer");

            //checkMessagesFromAndroid();


            $(document).keyup(function (e) {
                // CTRL + ALT + E
                // SR ENTRY
                if (e.ctrlKey && e.altKey && e.keyCode == 69) {
                    document.location = $('#entry').attr('href');
                }

                // CTRL + ALT + E
                // REPORTS
                if (e.ctrlKey && e.altKey && e.keyCode == 82) {
                    document.location = $('#reports').attr('href');
                }

                // CTRL + ALT + Q
                // BEATS CREATE
                if (e.ctrlKey && e.altKey && e.keyCode == 81) {
                    document.location = $('#beatCreateExcel').attr('href');
                }

                // CTRL + ALT + B
                // BEATS CREATE
                if (e.ctrlKey && e.altKey && e.keyCode == 66) {
                    document.location = $('#beatAssign').attr('href');
                }

                // CTRL + ALT + F
                // FORECAST
                if (e.ctrlKey && e.altKey && e.keyCode == 70) {
                    document.location = $('#checkForecast').attr('href');
                }
            });
        });

        function checkMessagesFromAndroid() {
            $.ajax({
                url: '/home/searchMessagesFromAndroid',
                data: { search: '', fromDate: null, toDate: null, read: 0 },
                success: function (data) {
                    var x = data.length;
                    if (x > 0) {
                        $('#messageCenter').css("color", "red");
                    }

                    $('#loader').fadeOut();
                },
                error: function () {

                    ShowMessage("ERROR", "error");
                    $('#loader').fadeOut();
                }
            });
        }
    </script>
