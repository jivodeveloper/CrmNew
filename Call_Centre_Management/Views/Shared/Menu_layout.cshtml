        $.extend($.expr[":"], {
            "containsIN": function (elem, i, match, array) {
                return (elem.textContent || elem.innerText || "").toLowerCase().indexOf((match[3] || "").toLowerCase()) >= 0;
            }
        });

        function mySearchFunction() {
            debugger;
            var search = $('#search').val();

            if (search == "") {
                $("li:contains(" + search + ")").css("background-color", "#2C3B41");
            }
            else {
                $("li:not(:containsIN(" + search + "))").css("background-color", "#2C3B41");
                $("a:not(:containsIN(" + search + "))").css("color", "#b8c7ce");
                $('li:containsIN("' + search + '")').css("background-color", "#3C8DBC");
                $("a:containsIN(" + search + ")").css({ "color": "white" });
                $("ul:containsIN(" + search + ")").css("background-color", "#2C3B41");
                $(".topBar").css("background-color", "#3C8DBC");
            }
        }
